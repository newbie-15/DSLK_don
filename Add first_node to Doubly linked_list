Method 1:
Code : 
#include <stdio.h>
#include <stdlib.h>
struct node{
	int data;
	struct node *next;
	struct node *prev;
};
typedef struct node node;
int len(node *head){
	int cnt = 0;
	node *tmp = head;
	while(tmp != NULL){
		++cnt;
		tmp = tmp->next;
	}
	return cnt;
}
node *makeNode(int x){
	node *newNode = (node*)malloc(sizeof(node));
	newNode->data = x;
	newNode->next = newNode->prev = NULL;
	return newNode;
}
void themdau(node **head, int x){
	node *newNode = makeNode(x);
	if(*head == NULL){
		newNode->next = *head;
		(*head) =newNode;
		return;
	}
	else{
		newNode->next = (*head);
		(*head)->prev = newNode;
		(*head) = newNode;
	}
}
void duyet(node *head){
	while(head != NULL){
		printf("%d ", head->data);
		head = head->next;
	}
}
int main(){
	node *head = NULL;
	for(int i = 1; i <= 10; i++){
		themdau(&head, i);
	}
	duyet(head);
	return 0;
}

Method 2:
Code:
#include <stdio.h>
#include <stdlib.h>
struct node{
	int data;
	struct node *next;
	struct node *prev;
};
typedef struct node node;
int len(node *head){
	int cnt = 0;
	node *tmp = head;
	while(tmp != NULL){
		++cnt;
		tmp = tmp->next;
	}
	return cnt;
}
node *makeNode(int x){
	node *newNode = (node*)malloc(sizeof(node));
	newNode->data = x;
	newNode->next = newNode->prev = NULL;
	return newNode;
}
node *themdau(node *head, int x){
	node *newNode = makeNode(x);
	if(head == NULL){
		newNode->next = head;
		head = newNode;
		return head;
	}
	else{
		newNode->next = head; //node moi tro vao head
		head->prev = newNode;//head tro nguoc ve node moi
		head = newNode; //cap nhat head la newnode
		return head; //do ta pass by value nen phai return head de bao rang da cap nhat head moi
	}
}
void duyet(node *head){
	while(head != NULL){
		printf("%d ", head->data);
		head = head->next;
	}
}
int main(){
	node *head = NULL;
	for(int i = 1; i <= 10; i++){
		head = themdau(head, i); //pass by value 
	}
	duyet(head);
	return 0;
}
